{
  "name": "codelab",
  "private": true,
  "scripts": {
    "serve": "node ./scripts/serve.js",
    "download:here": "node ./scripts/download-here.js",
    "generate:typeshed-data": "node ./scripts/generate-typeshed-data/index.js",
    "generate:config:qshell": "node ./scripts/generate-qshell-config.js",
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:skeleton": "pnpm -w -C apps/example dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.1.3",
    "typescript": "^5.4.5",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10.11.0"
  },
  "pnpm": {
    "overrides": {
      "@frontend/iris": "^0.16.2"
    }
  }
}
